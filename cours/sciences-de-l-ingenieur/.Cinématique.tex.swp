\section{Uniforme}
$\alpha = 0$
\begin{flalign*}
\alpha(t) &= 0\\
\omega(t) &= \omega_0\\
\theta(t) &= \omega_0 \cdot (t-t_0) + \theta_0
\end{flalign*}

\section{Uniformément varié}
$\alpha =  \text{constante}$
\begin{flalign*}
\alpha(t) &= \alpha_0\\
\omega(t) &= \alpha_0(t-t_0) + \omega_0\\
\theta(t) &= \frac{1}{2}\alpha_0(t-t_0)^2+\omega_0(t-t_0)+ \theta_0
\end{flalign*}

\section{Rapport de réduction $r$}
$Z_i$ : Nombre de dents (entrée, menante)\\
$Z_o$ : Nombre de dents (sortie, menée)
\begin{flalign*}
r = \frac{Z_{i}}{Z_{o}}=\frac{\omega_{o}}{\omega_{i}}
\end{flalign*}
